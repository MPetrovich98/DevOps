pipeline{
  agent any
  stages{
    stage('Hello'){
      steps {
        sh 'echo Hello World!'
      }
      post{
        success{
          srcipt{
            currentResult.result = 'FAILURE'
          }
        }
      }
    }
  }
  post{
    always{
      echo currentBuild.currentResult
    }
  }
}
